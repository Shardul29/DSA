package hello;
import java.util.*;

public class abc {
	
		public static void main(String[] args)
	{
		int arr[]= {5,9,1,11,8,6,15,3,12,2,10};
		PriorityQueue<Integer>pq=new PriorityQueue<>();
		//For changing the priority to Highest number.
		//PriorityQueue<Integer>pq=new PriorityQueue<>(Collections.reverseOrder());
		for(int val:arr) {
			pq.add(val);
		}
		//for printing elements priority-wise and removing them simultaneously 
		while(pq.size()>0) {
			System.out.println(pq.peek());
			pq.remove();
		}
		
	}
}

	   
