package trees;
import java.util.*;
public class test2 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a[]=new int[3];
		 a[0]=6;
		 a[1]=5;
		 a[2]=9;
		 System.out.println("enter a value for array index and divisio with the first index no.");
		 int ind=sc.nextInt();
		 int num=sc.nextInt();
		 try {
             System.out.println("No exception in level 1");
		try {
			System.out.println(a[ind]);
		    System.out.println("Division successful"+a[0]/num);
		}
		catch(ArrayIndexOutOfBoundsException e) {
			System.out.println("ArrayIndexOutOfBoundsException in level 2");
		}
		catch(ArithmeticException e) {
			System.out.println("Artihmetic Exception in level 2");
		}
		catch(Exception e) {
			System.out.println("Some exception in level 2 ");
		}
		 }
		 catch(Exception e) {
			 System.out.println("exception in level 1");
		 }
		 
		System.out.println("end of the program"); 
	}
}
	
